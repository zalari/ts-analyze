{
  "name": "@zalari/ts-analyze-cli",
  "version": "0.1.0",
  "description": "Infrastructure for analyzing TypeScript repository using static analysis: CLI tool.",
  "main": "./dist/main.js",
  "scripts": {
    "build": "npm run clean && tsc --project . && shx mkdir dist/bin && shx cp ./src/bin/ts-analyze ./dist/bin/",
    "clean": "shx rm -rf dist",
    "start": "node ./dist/main.js run-analyzer"
  },
  "author": "Zalari GmbH",
  "license": "MIT",
  "bin": {
    "ts-analyze": "./dist/bin/ts-analyze"
  },
  "dependencies": {
    "winston": "3.3.3",
    "@zalari/ts-analyze-base": "*",
    "@zalari/ts-analyze-analyzers-common": "*",
    "@zalari/ts-analyze-walkers-common": "*"
  },
  "devDependencies": {
    "@types/change-case": "2.3.1",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.3",
    "@types/yargs": "17.0.0",
    "change-case": "4.1.2",
    "shx": "0.3.3",
    "tslint": "6.1.3",
    "typedoc": "0.21.0",
    "typescript": "4.3.4",
    "yargs": "17.0.1"
  }
}
